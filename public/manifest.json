{
  "manifest_version": 3,
  "name": "Screen Time Tracker",
  "version": "1.0",
  "description": "Tracks screen time per tab and website",
  "permissions": ["tabs", "storage", "windows"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "src/popup/popup.html"
  }
}
